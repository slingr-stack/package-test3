{
   "label": "Package Test3",
   "name": "nesteddep1",
   "dependencies": [    {
     "type": "package",
     "name": "nesteddep2",
     "version": "v1.0.0"
   },{
     "type": "package",
     "name": "versionTest",
     "version": "v2.0.0"
   }],
   "configuration":[
     {
       "name": "message",
       "label": "Message for dependency",
       "type": "text",
       "required": false
     },
     {
       "name": "authenticationMethod",
       "label": "Authentication Method",
       "required": false,
       "defaultValue": "oAuth2",
       "type": "buttonsGroup",
       "typeOptions": {
         "allowCustom": false,
         "possibleValues":[
           {
             "label":"API Key",
             "name":"apiKey"
           },
           {
             "label":"OAuth 2.0",
             "name":"oAuth2"
           }
         ]
       }
     },
     {
       "name": "apiKey",
       "label": "API Key",
       "type": "text",
       "required": false,
       "visibility": "config.authenticationMethod == 'apiKey'"
     },
     {
       "name": "clientId",
       "label": "Client ID",
       "type": "text",
       "required": false,
       "visibility": "config.authenticationMethod == 'oAuth2'"
     },
     {
       "name": "clientSecret",
       "label": "Client secret",
       "type": "text",
       "required": false,
       "visibility": "config.authenticationMethod == 'oAuth2'"
     },
     {
       "name": "requestToken",
       "label": "Request token",
       "type": "oauth2",
       "visibility": "config.authenticationMethod == 'oAuth2'",
       "typeOptions": {
         "clientId": "config.clientId",
         "clientSecret": "config.clientSecret",
         "authUrl": "'https://app.pandadoc.com/oauth2/authorize'",
         "accessTokenUrl": "'https://api.pandadoc.com/oauth2/access_token'",
         "scope": "'read+write'",
         "grantType": "'authorization_code'",
         "accessToken": "@config.accessToken",
         "refreshToken": "@config.refreshToken"
       }
     },
     {
       "name": "accessToken",
       "label": "Access token",
       "type": "text",
       "visibility": "config.authenticationMethod == 'oAuth2'"
     },
     {
       "name": "refreshToken",
       "label": "Refresh token",
       "type": "text",
       "visibility": "config.authenticationMethod == 'oAuth2'"
     },
     {
       "name": "webhooksSharedKey",
       "label": "Webhooks shared key",
       "type": "text",
       "required": false
     },
     {
       "name": "webhook",
       "label": "Webhook URL",
       "type": "label",
       "value": "config.WEBHOOK_URL"
     }
   ],
   "events": [],
   "metadata": [
       {
           "type": "script",
           "namespace": "api",
           "path": "/scripts/other.js"
       },
     {
       "type": "script",
       "namespace": "api",
       "path": "/scripts/dependency.js"
     }
   ]
}