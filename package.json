{
   "label": "Package Test3",
   "name": "testdep",
   "dependencies": [    {
     "type": "package",
     "name": "packageTest",
     "version": "v2.1.3"
   },
     {
       "type": "package",
       "name": "packageTestt",
       "version": "v1.0.0"
     }],
   "configuration":[
        {
            "name": "authenticationMethod",
            "label": "Authentication Method",
            "required": true,
            "defaultValue": "oAuth2",
            "type": "buttonsGroup",
            "typeOptions": {
                "allowCustom": false,
                "possibleValues":[
                    {
                        "label":"API Key",
                        "name":"apiKey"
                    },
                    {
                        "label":"OAuth 2.0",
                        "name":"oAuth2"
                    }
                ]
            }
        },
        {
            "name": "apiKey",
            "label": "API Key",
            "type": "text",
            "required": "config.authenticationMethod == 'apiKey'",
            "visibility": "config.authenticationMethod == 'apiKey'"
        },
        {
            "name": "clientId",
            "label": "Client ID",
            "type": "text",
            "required": "config.authenticationMethod == 'oAuth2'",
            "visibility": "config.authenticationMethod == 'oAuth2'"
        },
        {
            "name": "clientSecret",
            "label": "Client secret",
            "type": "text",
            "required": "config.authenticationMethod == 'oAuth2'",
            "visibility": "config.authenticationMethod == 'oAuth2'"
        },
        {
            "name": "requestToken",
            "label": "Request token",
            "type": "oauth2",
            "visibility": "config.authenticationMethod == 'oAuth2'",
            "typeOptions": {
                "clientId": "config.clientId",
                "clientSecret": "config.clientSecret",
                "authUrl": "'https://app.pandadoc.com/oauth2/authorize'",
                "accessTokenUrl": "'https://api.pandadoc.com/oauth2/access_token'",
                "scope": "'read+write'",
                "grantType": "'authorization_code'",
                "accessToken": "@config.accessToken",
                "refreshToken": "@config.refreshToken"
            }
        },
        {
            "name": "accessToken",
            "label": "Access token",
            "type": "text",
            "visibility": "config.authenticationMethod == 'oAuth2'"
        },
        {
            "name": "refreshToken",
            "label": "Refresh token",
            "type": "text",
            "visibility": "config.authenticationMethod == 'oAuth2'"
        },
        {
            "name": "webhooksSharedKey",
            "label": "Webhooks shared key",
            "type": "text",
            "required": false
        },
        {
            "name": "webhook",
            "label": "Webhook URL",
            "type": "label",
            "value": "config.WEBHOOK_URL"
        }
    ],
   "events": [],
   "metadata": [
       {
           "type": "script",
           "namespace": "api",
           "path": "/scripts/test.js",
            "functions": ["t3","something"]
       },
     {
       "type": "script",
       "namespace": "api",
       "path": "/scripts/test2.js",
       "functions": ["aPlusB","otherTest"]
     }
   ]
}